<h1>Listing calendars</h1>

<table>
  <tr>
    <th>Data</th>
    <th>Season</th>
    <th>Week</th>
    <th>Day of week</th>
    <th>Rank</th>
    <th>Color</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calendars.each do |calendar| %>
  <tr>
    <td><%= calendar.data %></td>
    <td><%= calendar.season_id %></td>
    <td><%= calendar.week %></td>
    <td><%= calendar.day_of_week %></td>
    <td><%= calendar.rank_id %></td>
    <td><%= calendar.color_id %></td>
    <td><%= calendar.title %></td>
    <td><%= link_to 'Show', calendar %></td>
    <td><%= link_to 'Edit', edit_calendar_path(calendar) %></td>
    <td><%= link_to 'Destroy', calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Calendar', new_calendar_path %>
