<%= semantic_form_for @calendar do |f| %>
  <%= f.inputs do %>
    <%= f.input :data %>
    <%= f.input :season_id %>
    <%= f.input :week %>
    <%= f.input :day_of_week %>
    <%= f.input :rank_id %>
    <%= f.input :color_id %>
    <%= f.input :title %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
